    "silent": false,
    "skip_ntp": false,
    "skip_version_check": false,
    "swap": false,
    "timezone": "Europe/Berlin",
    "uki": true,
    "version": "2.7.1",
    "custom-commands": [
        "echo startx >/home/*/.bash_profile",
        "echo flameshot & >/home/*/.xinitrc",
        "echo exec emacs --fullscreen >>/home/*/.xinitrc",
        "mkdir -p /home/*/.emacs.d /home/*/.config/qutebrowser",
        "git clone https://github.com/port19x/dotfiles /home/*/dotfiles",
        "ln -sf /home/*/dotfiles/init.el /home/*/.emacs.d/init.el",
        "ln -sf /home/*/dotfiles/config.py /home/*/.config/qutebrowser/config.py",
        "emacs -l /home/*/.emacs.d/init.el -batch"
    ]
